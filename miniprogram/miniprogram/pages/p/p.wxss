.K{
  margin-left: 5%;
  width: 90%;
  border-radius: 35rpx;
  display: flex;
  margin-bottom: 10%;
  padding-bottom: 10%;
  border: solid;
}

.K1{
  width: 18%;
  margin-left: 2%;
  margin-top: 2%;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
  z-index: 1;
}

.circle{
  width: 100%;
  height: 120rpx;
  border-radius: 500px;
  border: 10rpx solid;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  z-index: 2;
  background-color: #FFFFFF;
}
.circle text{
  font-size: 35rpx;
  color: #101010;
  font-weight: bolder;
}
.L{
  width: 40%;
  height: 100%;
  border: solid;
  border-radius: 70rpx;
  padding-top:20% ;
